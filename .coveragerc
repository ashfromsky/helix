[run]
source = app
branch = True
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    app/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 2

[html]
directory = htmlcov
title = Helix Coverage Report